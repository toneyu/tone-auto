<Script name="Script 3 - VC Connectivity" apiVersion="4" automationXmlVersion="1">

  <Hosts>
    <Host>Host A</Host>
  </Hosts>

  <Steps>
    <!-- <Step type="feedback" name="Setup Feedbacks">
      <Feedbacks>
        <Feedback label="Mute">
          Status/Video/Selfview/Mode
        </Feedback>
      </Feedbacks>
    </Step> -->


    <Step type="putxml" name="Mute on">
      <Host>Host A</Host>
      <putxml>
        <Command>
          <Audio>
            <Microphones>
              <Mute>
              </Mute>
            </Microphones>
          </Audio>
        </Command>
      </putxml>
    </Step>

    <Step type="putxml" name="Unmute">
      <Host>Host A</Host>
      <putxml>
        <Command>
          <Audio>
            <Microphones>
              <Unmute>
              </Unmute>
            </Microphones>
          </Audio>
        </Command>
      </putxml>
    </Step>

    <Step type="putxml" name="Resume Call" description="Resume">
      <Host>Host A</Host>
      <putxml>
        <Command>
          <Call>
            <Resume></Resume>
          </Call>
        </Command>
      </putxml>
    </Step>

    <Step type="putxml" name="Disconnect Call">
      <Host>Host A</Host>
      <putxml>
        <Command>
          <Call>
            <Disconnect></Disconnect>
          </Call>
        </Command>
      </putxml>
    </Step>
  </Steps>
</Script>
